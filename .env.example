# OCTOPUS AI MULTI-AGENT SYSTEM CONFIGURATION

# APPLICATION
APP_NAME=Octopus
APP_VERSION=0.1.0
DEBUG=false
HOST=0.0.0.0
PORT=9527
LOG_LEVEL=INFO

# OPENAI-KEY
OPENAI_API_KEY=your-openai-key
OPENAI_BASE_URL=https://openrouter.ai/api/v1
OPENAI_MODEL=openai/gpt-4o
OPENAI_TEMPERATURE=0.7
OPENAI_MAX_TOKENS=4000

# ANP SYSTEM
ANP_SDK_ENABLED=true
ANP_GATEWAY_WS_URL=wss://anpproxy.com/ws
ANP_GATEWAY_HTTP_URL=www.anpproxy.com
# ANP RECEIVER (Local Instance)
ANP__RECEIVER__LOCAL_HOST=127.0.0.1
ANP__RECEIVER__TIMEOUT_SECONDS=30.0
ANP__RECEIVER__RECONNECT_ENABLED=true
ANP__RECEIVER__RECONNECT_DELAY=5.0
ANP__RECEIVER__MAX_RECONNECT_ATTEMPTS=10

# ANP AUTHENTICATION (强制开启)

# ANP TLS
ANP__RECEIVER__TLS__ENABLED=false
ANP__RECEIVER__TLS__VERIFY_MODE=required

# DID CONFIGURATION
DID_DOMAIN=didhost.cc
DID_PATH=test:public
DID_DOCUMENT_PATH=docs/user_public/did.json
DID_PRIVATE_KEY_PATH=docs/user_public/key-1_private.pem
AUTH_NONCE_EXPIRY_MINUTES=5
AUTH_TIMESTAMP_EXPIRY_MINUTES=5

# JWT AUTHENTICATION
JWT_ALGORITHM=RS256
ACCESS_TOKEN_EXPIRE_MINUTES=60
JWT_PRIVATE_KEY_PATH=docs/jwt_key/private_key.pem
JWT_PUBLIC_KEY_PATH=docs/jwt_key/public_key.pem
